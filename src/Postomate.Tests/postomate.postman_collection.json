							"listen": "prerequest",
							"script": {
								"exec": [
									"pm.variables.set(\"firstName\", \"Arthur\")\r",
									"pm.variables.set(\"surname\", \"Dent\")"
								],
								"type": "text/javascript"
							}
						},
						{
							"listen": "test",
							"script": {
								"exec": [
									"var payload = pm.response.json();\r",
									"\r",
									"pm.test(\"firstName can be persisted\", () => {\r",
									"    pm.expect(payload.firstName).to.be.eq(pm.variables.get(\"surname\"));\r",
									"});\r",
									"\r",
									"pm.test(\"surname can be persisted\", () => {\r",
									"    pm.expect(payload.surname, \"the surname should match\").to.be.eq(pm.variables.get(\"surname\"));\r",
									"});"
								],
								"type": "text/javascript"
							}
						}
